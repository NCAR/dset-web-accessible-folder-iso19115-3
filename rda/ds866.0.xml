          <cit:title><gco:CharacterString>GISS Global Methane and Livestock Distribution, 1-Degree</gco:CharacterString></cit:title>
          <cit:date>
            <cit:CI_Date>
              <cit:date><gco:DateTime>1989-07-20T00:00:00-07:00</gco:DateTime></cit:date>
              <cit:dateType><cit:CI_DateTypeCode codeList="http://standards.iso.org/iso/19115/-3/cit/1.0/codelists.html#CI_DateTypeCode" codeListValue="publication">publication</cit:CI_DateTypeCode></cit:dateType>
